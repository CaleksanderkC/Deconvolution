function q = charge_output(d, T_smp, tau, Cfeed)

    % t_0 przewidywane z oblicze≈Ñ     
    t_0=(d(:,2)./d(:,1)*T_smp)./(d(:,2)./d(:,1)+exp(-T_smp/tau));
    
    % V_max
    A=(d(:,1)+d(:,2)).*(tau*exp((T_smp-t_0-tau)/tau)./(T_smp-t_0*(1-exp(-T_smp/tau))));

    q=A*exp(1)*Cfeed;
    
    if isnan(q(:))
        disp('Error Charge_out')
    end
end